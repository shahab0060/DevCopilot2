@inject IViewLocalizer Localizer
@inject IHtmlLocalizer<SharedResources> SharedLocalizer
@inject IHtmlLocalizer<EntitiesSharedResources> SharedEntitiesLocalizer
@using System
@using System.ComponentModel.DataAnnotations
@using DevCopilot2.Domain.DTOs.Entities
@using DevCopilot2.Domain.DTOs.Properties
@using DevCopilot2.Domain.DTOs.Imports
@using DevCopilot2.Domain.DTOs.Languages
@using DevCopilot2.Domain.Enums.DataTypes
@using DevCopilot2.Domain.Enums.Relations
@model ImportEntitiesFromDbDto
@{
    string title = $"{SharedLocalizer.GetString("Modify And Review Entities")}";
    List<SelectListItem> entities = ViewData["Entities"] as List<SelectListItem>;
    List<LanguageListDto> languagesList = ViewData["LanguagesList"] as List<LanguageListDto>;
    List<SelectListItem> languages = ViewData["Languages"] as List<SelectListItem>;
    List<SelectListItem> projectAreas = ViewData["ProjectAreas"] as List<SelectListItem>;
    List<SelectListItem> projectEnums = ViewData["ProjectEnums"] as List<SelectListItem>;
    CreatePropertyDto createProperty = new CreatePropertyDto();
    CreateEntitySelectedProjectAreaDto createEntitySelectedProjectArea = new CreateEntitySelectedProjectAreaDto();
    CreateEntitySelectedLanguageDto createEntitySelectedLanguage = new CreateEntitySelectedLanguageDto();
    CreateEntityRelationDto createEntityRelation = new CreateEntityRelationDto();
    CreatePropertyImageResizeInformationDto createPropertyImageResizeInformation = new CreatePropertyImageResizeInformationDto();
    CreatePropertySelectedLanguageDto createPropertySelectedLanguage = new CreatePropertySelectedLanguageDto();
    CreateEntitySelectedProjectAreaSelectedFilterDto createEntitySelectedProjectAreaSelectedFilter = new CreateEntitySelectedProjectAreaSelectedFilterDto();
}

<nav class="mb-3" aria-label="breadcrumb">
    <ol class="breadcrumb mb-0">
        <li class="breadcrumb-item">
            <a asp-area="Admin"
               asp-controller="Home"
               asp-action="Index"
               title="@SharedLocalizer.GetString("Admin")">@SharedLocalizer.GetString("Admin")</a>
        </li>
        <li class="breadcrumb-item">
            <a asp-area="Admin"
               asp-controller="EntityDbImport"
               asp-action="SelectProject"
               title="@SharedLocalizer.GetString("SelectProject")">@SharedLocalizer.GetString("SelectProject")</a>
        </li>
        <li class="breadcrumb-item">
            <a asp-area="Admin"
               asp-controller="EntityDbImport"
               asp-action="Index"
               title="@SharedLocalizer.GetString("Import Entities From Db")">@SharedLocalizer.GetString("Import Entities From Db")</a>
        </li>
        <li class="breadcrumb-item active">@title</li>
    </ol>
</nav>
<form class="mb-9" method="post"
      asp-area="Admin" asp-controller="EntityDbImport" asp-action="SelectEntities"
      enctype="multipart/form-data">

    <div class="row justify-content-between align-items-end g-3 mb-5">
        <div class="col-12 col-sm-auto col-xl-8">
            <h2 class="mb-0">@title</h2>
        </div>
        <div class="col-12 col-sm-auto col-xl-4">
            <div class="d-flex">
                <button class="btn btn-primary px-5 w-100 text-nowrap">@SharedLocalizer.GetString("Continue")</button>
            </div>
        </div>
    </div>
    <div class="row g-5">
        <input type="hidden" asp-for="@Model.ProjectId" />
        <input type="hidden" asp-for="@Model.AddDefaultValueLanguages" />
        <input type="hidden" asp-for="@Model.ConnectionString.DataSource" />
        <input type="hidden" asp-for="@Model.ConnectionString.DbName" />
        <input type="hidden" asp-for="@Model.ConnectionString.UserName" />
        <input type="hidden" asp-for="@Model.ConnectionString.Password" />
        <input type="hidden" asp-for="@Model.ConnectionString.MultipleActiveResultsSet" />
        <input type="hidden" asp-for="@Model.ConnectionString.TrustServerCertificate" />
        @if (Model.ExcludePropertiesNamesList is not null)
        {
            for (int i = 0; i < Model.ExcludePropertiesNamesList.Count; i++)
            {
                <input type="hidden" asp-for="@Model.ExcludePropertiesNamesList[i]" />
            }
        }
        <div class="sub-form-container" data-name="EntitiesList">
            <h3>@SharedEntitiesLocalizer.GetString("Entities")</h3>
            @for (int entityIndex = 0; entityIndex < Model.EntitiesList.Count; entityIndex++)
            {
                string entityDataName = "EntitiesList[" + entityIndex + "]";
                <h3 class="collapsible-container" data-name="@("EntitiesList")">@Model.EntitiesList[entityIndex].PluralName.AddSpacesBetweenCapitals()</h3>
                <div class="single-sub-form row g-5" data-name="@("EntitiesList")">
                    <div class="remove-btn-container" data-name="@($"EntitiesList[{entityIndex}]")">
                        <div class="d-flex justify-content-end mt-3">
                            <p class="btn btn-danger next-btn mb-0 remove-sub-item-btn"
                               data-name="@("EntitiesList")"
                               onclick="RemoveSubItem(this)">@SharedLocalizer.GetString("Delete") @SharedEntitiesLocalizer.GetString("Entity")</p>
                        </div>
                    </div>
                    <input type="hidden" asp-for="@Model.EntitiesList[entityIndex].ProjectId"
                           class="sub-form-input"
                           data-name="EntitiesList"
                           data-prefix="EntitiesList"
                           data-suffix="ProjectId" />
                    <div class="col-sm-12 col-md-6">
                        <div class="form-floating">
                            <input class="form-control Title-input singular-name-input" asp-for="@Model.EntitiesList[entityIndex].SingularName" placeholder="" />
                            <label asp-for="@Model.EntitiesList[entityIndex].SingularName"></label>
                        </div>
                        <span asp-validation-for="@Model.EntitiesList[entityIndex].SingularName"></span>
                    </div>
                    <div class="col-sm-12 col-md-6">
                        <div class="form-floating">
                            <input class="form-control Title-input" asp-for="@Model.EntitiesList[entityIndex].PluralName" placeholder="" />
                            <label asp-for="@Model.EntitiesList[entityIndex].PluralName"></label>
                        </div>
                        <span asp-validation-for="@Model.EntitiesList[entityIndex].PluralName"></span>
                    </div>
                    <div class="col-sm-12 col-md-6">
                        <div class="form-floating">
                            <input class="form-control Title-input" asp-for="@Model.EntitiesList[entityIndex].FolderName" placeholder="" />
                            <label asp-for="@Model.EntitiesList[entityIndex].FolderName"></label>
                        </div>
                        <span asp-validation-for="@Model.EntitiesList[entityIndex].FolderName"></span>
                    </div>
                    <div class="col-sm-12 col-md-6">
                        <div class="form-floating">
                            <select class="form-select" asp-for="@Model.EntitiesList[entityIndex].InheritedEntityId" asp-items="entities">
                            </select>
                            <label asp-for="@Model.EntitiesList[entityIndex].InheritedEntityId"></label>
                        </div>
                        <span asp-validation-for="@Model.EntitiesList[entityIndex].InheritedEntityId"></span>
                    </div>
                    <div class="col-sm-12 col-md-6">
                        <div class="form-floating">
                            <select class="form-select" asp-for="@Model.EntitiesList[entityIndex].IdType">

                                @foreach (IdTypeEnum idTypeEnum in (IdTypeEnum[])Enum.GetValues(typeof(IdTypeEnum)))
                                {
                                    <option value="@idTypeEnum">@idTypeEnum.GetEnumName()</option>
                                }
                            </select>
                            <label asp-for="@Model.EntitiesList[entityIndex].IdType"></label>
                        </div>
                        <span asp-validation-for="@Model.EntitiesList[entityIndex].IdType"></span>
                    </div>
                    <div class="col-sm-12 col-md-6">
                        <div class="form-floating">
                            <input class="form-control Others-input" asp-for="@Model.EntitiesList[entityIndex].ServiceName" placeholder="" />
                            <label asp-for="@Model.EntitiesList[entityIndex].ServiceName"></label>
                        </div>
                        <span asp-validation-for="@Model.EntitiesList[entityIndex].ServiceName"></span>
                    </div>
                    <input type="hidden" asp-for="@Model.EntitiesList[entityIndex].AuthorId" value="1" class="" />
                    <div class="col-sm-4">
                        <div class="form-check">
                            <input class="form-check-input Others-input" asp-for="@Model.EntitiesList[entityIndex].IsExcluded" type="checkbox" />
                            <label class="form-check-label fw-bold fs-8 mb-5" asp-for="@Model.EntitiesList[entityIndex].IsExcluded"></label>
                        </div>
                    </div>
                    <div class="col-sm-4">
                        <div class="form-check">
                            <input class="form-check-input Title-input" asp-for="@Model.EntitiesList[entityIndex].AddToMenu" type="checkbox" />
                            <label class="form-check-label fw-bold fs-8 mb-5" asp-for="@Model.EntitiesList[entityIndex].AddToMenu"></label>
                        </div>
                    </div>

                    <div class="sub-form-container" data-name="@("EntitySelectedProjectAreasList")">
                        <h3>@SharedEntitiesLocalizer.GetString("EntitySelectedProjectAreas")</h3>
                        @if (Model.EntitiesList[entityIndex].EntitySelectedProjectAreasList is not null)
                        {
                            for (int entitySelectedProjectArea = 0; entitySelectedProjectArea < Model.EntitiesList[entityIndex].EntitySelectedProjectAreasList.Count; entitySelectedProjectArea++)
                            {

                                string entitySelectedProjectAreaDataName = "EntitySelectedProjectAreasList" + "[" + entitySelectedProjectArea + "]";

                                <div class="single-sub-form row g-5" data-name="@("EntitySelectedProjectAreasList")">
